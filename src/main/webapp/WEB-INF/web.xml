<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>cntLogin</servlet-name>
        <servlet-class>ServLets.cntLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cntLogout</servlet-name>
        <servlet-class>ServLets.cntLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cntCustomer</servlet-name>
        <servlet-class>ServLets.cntCustomer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cntLocales</servlet-name>
        <servlet-class>ServLets.cntLocales</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cntReclamos</servlet-name>
        <servlet-class>ServLets.cntReclamos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cntLogin</servlet-name>
        <url-pattern>/cntLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cntLogout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cntCustomer</servlet-name>
        <url-pattern>/cntCustomer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cntLocales</servlet-name>
        <url-pattern>/cntLocales</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cntReclamos</servlet-name>
        <url-pattern>/cntReclamos</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Inicio.jsp</welcome-file>
    </welcome-file-list>
</web-app>
